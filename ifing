Program decisiones;
VARS
    int: x, y, z, a;

principal(){
    x = 10;
    y = 0;
    z = 2; 
    if(x > y) then {
        y = y + 5;
        write("nueva y: ", y);
    }
    if(y <> 0) then {
        y = 0;
        write("Y returned to 0");
    }
    if(y == 0 and z >= x) then {
        write("Siempre Falso");
    }else{
        write("ELSE GET HERE");
    }

}