Program funcS;
VARS
    int: a, b, c;
    float: d, e;

function void calcula (float: x);
VARS int: y;
{
    y = 1;
    while(y < 15) do {
        x = x * d;
        y = y + 1;
    }
    write("Acumulado en funcion void", x);
}


principal(){
    a = 3;
    d = 1.2;
    write(a, d);
    calcula(10.0)
}